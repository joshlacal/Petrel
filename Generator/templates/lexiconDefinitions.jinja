{%- for name, def_schema in lex_definitions.items() %}
        public struct {{ name }}{{ def_schema.conformance }} {
            public static let typeIdentifier = "{{ lexicon_id }}#{{ name|lowerCamelCase }}"

        {%- for prop in def_schema.properties %}
            {%- if prop.boxed %}
            // Boxed to break circular reference
            private let _{{ prop.name }}: IndirectBox<{{ prop.type }}>{% if prop.optional %}?{% endif %}
            public var {{ prop.name }}: {{ prop.type }}{% if prop.optional %}?{% endif %} {
                {% if prop.optional %}
                _{{ prop.name }}?.value
                {%- else %}
                _{{ prop.name }}.value
                {%- endif %}
            }
            {%- else %}
            public let {{ prop.name }}: {{ prop.type }}{% if prop.optional %}?{% endif %}
            {%- endif %}
        {%- endfor %}

        // Standard initializer
        public init(
            {% for prop in def_schema.properties -%}
            {{ prop.name }}: {{ prop.type }}{% if prop.optional %}?{% endif %}{% if not loop.last %}, {% endif %}
            {%- endfor %}
        ) {
            {% for prop in def_schema.properties %}
            {%- if prop.boxed %}
            {% if prop.optional %}
            self._{{ prop.name }} = {{ prop.name }}.map { IndirectBox($0) }
            {%- else %}
            self._{{ prop.name }} = IndirectBox({{ prop.name }})
            {%- endif %}
            {%- else %}
            self.{{ prop.name }} = {{ prop.name }}
            {%- endif %}
            {%- endfor %}
        }

        // Codable initializer
        public init(from decoder: Decoder) throws {
            {% if def_schema.properties %}
            let container = try decoder.container(keyedBy: CodingKeys.self)
            {%- for prop in def_schema.properties %}
            do {
                {% if prop.boxed %}
                {% if prop.optional %}
                let decoded = try container.decodeIfPresent({{ prop.type }}.self, forKey: .{{ prop.name }})
                self._{{ prop.name }} = decoded.map { IndirectBox($0) }
                {% else %}
                let decoded = try container.decode({{ prop.type }}.self, forKey: .{{ prop.name }})
                self._{{ prop.name }} = IndirectBox(decoded)
                {% endif %}
                {% else %}
                {% if prop.optional %}
                self.{{ prop.name }} = try container.decodeIfPresent({{ prop.type }}.self, forKey: .{{ prop.name }})
                {% else %}
                self.{{ prop.name }} = try container.decode({{ prop.type }}.self, forKey: .{{ prop.name }})
                {% endif %}
                {% endif %}
            } catch {
                {% if prop.optional %}
                LogManager.logDebug("Decoding error for optional property '{{ prop.name }}': \(error)")
                {% else %}
                LogManager.logError("Decoding error for required property '{{ prop.name }}': \(error)")
                {% endif %}
                throw error
            }
            {%- endfor %}
            {% else %}
            let _ = decoder  // Acknowledge parameter for empty struct
            {% endif %}
        }

        public func encode(to encoder: Encoder) throws {
            var container = encoder.container(keyedBy: CodingKeys.self)
            try container.encode(Self.typeIdentifier, forKey: .typeIdentifier)
            {%- for prop in def_schema.properties %}
            {% if prop.boxed %}
            {% if prop.optional %}
            // Encode boxed optional property
            try container.encodeIfPresent(_{{ prop.name }}?.value, forKey: .{{ prop.name }})
            {% else %}
            // Encode boxed property
            try container.encode(_{{ prop.name }}.value, forKey: .{{ prop.name }})
            {% endif %}
            {% else %}
            {% if prop.optional %}
            // Encode optional property even if it's an empty array
            try container.encodeIfPresent({{ prop.name }}, forKey: .{{ prop.name }})
            {% else %}
            try container.encode({{ prop.name }}, forKey: .{{ prop.name }})
            {% endif %}
            {% endif %}
            {%- endfor %}
        }

        public func hash(into hasher: inout Hasher) {
            {%- for prop in def_schema.properties %}
            {%- if prop.boxed %}
            {%- if prop.optional %}
            if let value = _{{ prop.name }}?.value {
                hasher.combine(value)
            } else {
                hasher.combine(nil as Int?)
            }
            {%- else %}
            hasher.combine(_{{ prop.name }}.value)
            {%- endif %}
            {%- else %}
            {%- if prop.optional %}
            if let value = {{ prop.name }} {
                hasher.combine(value)
            } else {
                hasher.combine(nil as Int?)
            }
            {%- else %}
            hasher.combine({{ prop.name }})
            {%- endif %}
            {%- endif %}
            {%- endfor %}
        }

        public func isEqual(to other: any ATProtocolValue) -> Bool {
            {% if def_schema.properties %}
            guard let other = other as? Self else { return false }
            {%- for prop in def_schema.properties %}
            {% if prop.boxed %}
            {% if prop.optional %}
            if _{{ prop.name }}?.value != other._{{ prop.name }}?.value {
                return false
            }
            {% else %}
            if _{{ prop.name }}.value != other._{{ prop.name }}.value {
                return false
            }
            {% endif %}
            {% else %}
            {% if prop.optional %}
            if {{ prop.name }} != other.{{ prop.name }} {
                return false
            }
            {% else %}
            if self.{{ prop.name }} != other.{{ prop.name }} {
                return false
            }
            {% endif %}
            {% endif %}
            {%- endfor %}
            return true
            {% else %}
            return other is Self  // For empty structs, just check the type
            {% endif %}
        }

        public static func == (lhs: Self, rhs: Self) -> Bool {
            return lhs.isEqual(to: rhs)
        }

        // DAGCBOR encoding with field ordering
        public func toCBORValue() throws -> Any {
            var map = OrderedCBORMap()

            map = map.adding(key: "$type", value: Self.typeIdentifier)

            {% for prop in def_schema.properties %}
            {% if prop.boxed %}
            {% if prop.optional %}
            if let value = _{{ prop.name }}?.value {
                // Encode boxed optional property for CBOR
                {% set var_name = prop.name + 'Value' %}
                let {{ var_name }} = try value.toCBORValue()
                map = map.adding(key: "{{ prop.name }}", value: {{ var_name }})
            }
            {% else %}
            // Encode boxed property for CBOR
            {% set var_name = prop.name + 'Value' %}
            let {{ var_name }} = try _{{ prop.name }}.value.toCBORValue()
            map = map.adding(key: "{{ prop.name }}", value: {{ var_name }})
            {% endif %}
            {% else %}
            {% if prop.optional %}
            if let value = {{ prop.name }} {
                // Encode optional property even if it's an empty array for CBOR
                {% set var_name = prop.name + 'Value' %}
                let {{ var_name }} = try value.toCBORValue()
                map = map.adding(key: "{{ prop.name }}", value: {{ var_name }})
            }
            {% else %}
            {% set var_name = prop.name + 'Value' %}
            let {{ var_name }} = try {{ prop.name }}.toCBORValue()
            map = map.adding(key: "{{ prop.name }}", value: {{ var_name }})
            {% endif %}
            {% endif %}
            {% endfor %}

            return map
        }

        private enum CodingKeys: String, CodingKey {
            case typeIdentifier = "$type"
            {%- for prop in def_schema.properties %}
            case {{ prop.name }}
            {%- endfor %}
        }

        {%- for sub_struct_name, sub_struct in def_schema.sub_structs.items() %}
        public struct {{ sub_struct_name }}{{ sub_struct.conformance }} {
            public static let typeIdentifier = "{{ lexicon_id }}#{{ sub_struct_name|lowerCamelCase }}"

            {%- for prop in sub_struct.properties %}
            public let {{ prop.name }}: {{ prop.type }}{% if prop.optional %}?{% endif %}
            {%- endfor %}

            // Standard initializer for sub-struct
            public init({% for prop in sub_struct.properties -%}
                {{ prop.name }}: {{ prop.type }}{% if prop.optional %}?{% endif %}{% if not loop.last %}, {% endif %}
                {%- endfor %}) {
                {% for prop in sub_struct.properties -%}
                self.{{ prop.name }} = {{ prop.name }}
                {%- endfor %}
            }

            // Codable initializer for sub-struct
            public init(from decoder: Decoder) throws {
                {% if sub_struct.properties %}
                let container = try decoder.container(keyedBy: CodingKeys.self)
                {%- for prop in sub_struct.properties %}
                do {
                    {% if prop.optional %}
                    self.{{ prop.name }} = try container.decodeIfPresent({{ prop.type }}.self, forKey: .{{ prop.name }})
                    {% else %}
                    self.{{ prop.name }} = try container.decode({{ prop.type }}.self, forKey: .{{ prop.name }})
                    {% endif %}
                } catch {
                    {% if prop.optional %}
                    LogManager.logDebug("Decoding error for optional sub-struct property '{{ prop.name }}': \(error)")
                    {% else %}
                    LogManager.logError("Decoding error for required sub-struct property '{{ prop.name }}': \(error)")
                    {% endif %}
                    throw error
                }
                {%- endfor %}
                {% else %}
                let _ = decoder  // Acknowledge parameter for empty struct
                {% endif %}
            }

            public func encode(to encoder: Encoder) throws {
                var container = encoder.container(keyedBy: CodingKeys.self)
                // Encode the $type field for sub-struct
                try container.encode(Self.typeIdentifier, forKey: .typeIdentifier)
                {%- for prop in sub_struct.properties %}
                {% if prop.optional %}
                // Encode optional property even if it's an empty array
                try container.encodeIfPresent({{ prop.name }}, forKey: .{{ prop.name }})
                {% else %}
                try container.encode({{ prop.name }}, forKey: .{{ prop.name }})
                {% endif %}
                {%- endfor %}
            }

            public func hash(into hasher: inout Hasher) {
                {%- for prop in sub_struct.properties %}
                {%- if prop.optional %}
                if let value = {{ prop.name }} {
                    hasher.combine(value)
                } else {
                    hasher.combine(nil as Int?)
                }
                {%- else %}
                hasher.combine({{ prop.name }})
                {%- endif %}
                {%- endfor %}
            }

            public func isEqual(to other: any ATProtocolValue) -> Bool {
                {% if sub_struct.properties %}
                guard let other = other as? Self else { return false }
                {%- for prop in sub_struct.properties %}
                {% if prop.optional %}
                if {{ prop.name }} != other.{{ prop.name }} {
                    return false
                }
                {% else %}
                if self.{{ prop.name }} != other.{{ prop.name }} {
                    return false
                }
                {% endif %}
                {%- endfor %}
                return true
                {% else %}
                return other is Self  // For empty structs, just check the type
                {% endif %}
            }

            public static func == (lhs: Self, rhs: Self) -> Bool {
                return lhs.isEqual(to: rhs)
            }

            // DAGCBOR encoding with field ordering for sub-struct
            public func toCBORValue() throws -> Any {
                var map = OrderedCBORMap()

                map = map.adding(key: "$type", value: Self.typeIdentifier)

                {% for prop in sub_struct.properties %}
                {% if prop.optional %}
                if let value = {{ prop.name }} {
                    // Encode optional property even if it's an empty array for CBOR
                    {% set var_name = prop.name + 'Value' %}
                    let {{ var_name }} = try value.toCBORValue()
                    map = map.adding(key: "{{ prop.name }}", value: {{ var_name }})
                }
                {% else %}
                {% set var_name = prop.name + 'Value' %}
                let {{ var_name }} = try {{ prop.name }}.toCBORValue()
                map = map.adding(key: "{{ prop.name }}", value: {{ var_name }})
                {% endif %}
                {% endfor %}

                return map
            }

            private enum CodingKeys: String, CodingKey {
                case typeIdentifier = "$type"
                {%- for prop in sub_struct.properties %}
                case {{ prop.name }}
                {%- endfor %}
            }
        }
        {%- endfor %}
    }
    {%- endfor %}
