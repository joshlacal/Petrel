{% for def in definitions %}
{% if def.type == 'data_class' %}
{% if def.description %}
    /**
     * {{ def.description }}
     */
{% endif %}
    @Serializable
    {% if def.properties %}
    data class {{ def.name }}(
        {% for prop in def.properties %}
        {% if prop.description %}/** {{ prop.description }} */{% endif %}
        @SerialName("{{ prop.name }}")
        val {{ prop.name | sanitizeKeyword }}: {{ prop.type }}{% if not loop.last %},{% endif %}
        {% endfor %}
    ) {
        companion object {
            const val TYPE_IDENTIFIER = "{{ lexicon_id }}#{{ def.name | camelCase }}"
        }
    }
    {% else %}
    class {{ def.name }} {
        companion object {
            const val TYPE_IDENTIFIER = "{{ lexicon_id }}#{{ def.name | camelCase }}"
        }
    }
    {% endif %}

{% elif def.type == 'type_alias' %}
{% if def.description %}
    /**
     * {{ def.description }}
     */
{% endif %}
    typealias {{ def.name }} = {{ def.target }}

{% endif %}
{% endfor %}
