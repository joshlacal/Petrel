@Serializable
{% if properties %}
data class {{ class_name }}(
    {% for prop in properties %}
    {% if prop.description %}// {{ prop.description }}{% endif %}
    @SerialName("{{ prop.name }}")
    val {{ prop.name | sanitizeKeyword }}: {{ prop.type }}{% if not loop.last %},{% endif %}
    {% endfor %}
)
{% else %}
class {{ class_name }}
{% endif %}
